[core]
	excludesfile = /home/kurtosis/.gitignore_global
	editor = vim
[alias]
#   unstage = reset HEAD
    unstage = rm -r --cached
    pull = pull -u
    push = push -u
    log = log --decorate --graph --oneline
    lol = log --decorate --graph --oneline --abbrev-commit
    lola = log --graph --decorate --oneline --abbrev-commit --all
[merge]
	tool = vimdiff
	conflictstyle = diff3
[color]
	ui = auto
